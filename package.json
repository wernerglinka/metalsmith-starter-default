{
  "name": "metalsmith-starter-default",
  "version": "0.0.1",
  "description": "A bare-bones Metalsmith starter",
  "keywords": [
    "metalsmith",
    "starter"
  ],
  "author": "werner@glinka.co",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development NODE_PATH=./node_modules npm run watch & npm run serve",
    "build": "cross-env NODE_ENV=production node metalsmith.js",
    "watch": "nodemon metalsmith.js",
    "serve": "browser-sync start --server 'build' --files 'build/**'"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "templates"
    ],
    "ext": "js scss njk json html md"
  },
  "devDependencies": {
    "browser-sync": "^2.27.7",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "@metalsmith/drafts": "^1.1.1",
    "@metalsmith/in-place": "^4.5.0",
    "@metalsmith/layouts": "^2.4.0",
    "@metalsmith/permalinks": "^2.4.0",
    "jstransformer-markdown": "^1.2.1",
    "jstransformer-nunjucks": "^1.0.0",
    "marked": "^4.0.12",
    "metalsmith": "^2.4.1",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-debug-ui": "^0.3.2",
    "metalsmith-html-minifier": "^4.0.1",
    "metalsmith-if": "^0.1.1",
    "metalsmith-metadata": "^0.0.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
